    "def square_num(n):\n",
    "    return n * n\n",
    "nums = [4, 5, 2, 9]\n",
    "print(\"Sample List: \",nums)\n",
    "result = map(square_num, nums)\n",
    "print(\"\\nSquare the elements of the list :\\n\")\n",
    "print(list(result))\n"
